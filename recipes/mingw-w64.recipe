    commit = 'sdk-release-sdk-2012.5'
    licenses = [License.LGPL]
    configure_options = '--without-headers --with-tools=gendef --with-sysroot=$CERBERO_PREFIX'
    add_host_build_target = True
    deps = ['mingw-w64-headers', 'binutils', 'gcc-core']

